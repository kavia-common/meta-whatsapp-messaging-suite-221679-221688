# Frontend environment variables (copy to .env and adjust)
REACT_APP_API_BASE=/api
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=
# WebSocket endpoint (ws:// or wss://). Leave empty to disable WS features (fallback to polling).
REACT_APP_WS_URL=ws://localhost:3001/ws
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=1
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=0
REACT_APP_LOG_LEVEL=debug
REACT_APP_HEALTHCHECK_PATH=/healthz
# JSON object of feature flags, e.g. {"newUI":true}
REACT_APP_FEATURE_FLAGS={}
REACT_APP_EXPERIMENTS_ENABLED=0
